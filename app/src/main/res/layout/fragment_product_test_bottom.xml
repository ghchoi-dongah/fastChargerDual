<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="1024dp"
    android:layout_height="168dp"
    tools:context=".pages.ProductTestBottomFragment">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/btnExit"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:text="종 료"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:textSize="32sp"
        android:background="@drawable/selector_default_btn"
        android:layout_marginStart="30dp"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnIO"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:text="IO Test"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:textSize="30sp"
        android:background="@drawable/selector_default_btn"
        android:layout_marginStart="100dp"
        app:layout_constraintLeft_toRightOf="@id/btnExit"
        app:layout_constraintTop_toTopOf="@id/btnExit" />


    <Button
        android:id="@+id/btnLoad"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:text="개별 부하 Test"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_default_btn"
        android:layout_marginStart="50dp"
        app:layout_constraintLeft_toRightOf="@id/btnIO"
        app:layout_constraintTop_toTopOf="@id/btnIO" />

    <Button
        android:id="@+id/btnTotalLoad"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:text="부하 Test"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_default_btn"
        android:layout_marginStart="50dp"
        app:layout_constraintLeft_toRightOf="@id/btnLoad"
        app:layout_constraintTop_toTopOf="@id/btnLoad" />


</androidx.constraintlayout.widget.ConstraintLayout>