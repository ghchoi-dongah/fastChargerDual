<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="1024dp"
    android:layout_height="600dp"
    tools:context=".pages.ProductTestLoadFragment">

    <TextView
        android:id="@+id/txtTitle1"
        android:layout_width="512dp"
        android:layout_height="wrap_content"
        android:textSize="36sp"
        android:textAlignment="center"
        android:layout_marginTop="10dp"
        android:text="LOAD TEST (CH1)"
        android:background="@drawable/shape_text_title"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnStart1"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:text=" Start"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_confirm_btn"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/txtTitle1"
        app:layout_constraintLeft_toLeftOf="@id/txtTitle1" />

    <Button
        android:id="@+id/btnStop1"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:text=" Stop"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_stop_btn"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:enabled="false"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@id/txtTitle1"
        app:layout_constraintLeft_toRightOf="@id/btnStart1" />

    <TextView
        android:id="@+id/txtOutV1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="출력전압 (V)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtDrA1"
        app:layout_constraintLeft_toLeftOf="@id/btnStart1" />

    <EditText
        android:id="@+id/editOutV1"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:gravity="right"
        android:textColor="#000000"
        android:focusable="false"
        android:background="@drawable/shape_edit_text_disenable"
        app:layout_constraintLeft_toRightOf="@id/txtOutV1"
        app:layout_constraintBottom_toBottomOf="@id/txtOutV1" />

    <TextView
        android:id="@+id/txtOutA1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="출력전류 (A)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtOutV1"
        app:layout_constraintLeft_toLeftOf="@id/txtOutV1" />

    <EditText
        android:id="@+id/editOutA1"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:gravity="right"
        android:textColor="#000000"
        android:focusable="false"
        android:background="@drawable/shape_edit_text_disenable"
        app:layout_constraintLeft_toRightOf="@id/txtOutA1"
        app:layout_constraintBottom_toBottomOf="@id/txtOutA1" />

    <ToggleButton
        android:id="@+id/btnMainMC1"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:textOff="Main MC off"
        android:textOn="Main MC on"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/editOutA1"
        app:layout_constraintLeft_toLeftOf="@id/editOutA1" />

    <TextView
        android:id="@+id/txtDrV1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전압지령 (V)"
        android:textSize="24sp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/btnStart1"
        app:layout_constraintLeft_toLeftOf="@id/btnStart1" />

    <EditText
        android:id="@+id/editDrV1"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:text="0"
        android:gravity="right"
        android:textColor="#000000"
        android:background="@drawable/shape_edit_text_focus"
        app:layout_constraintLeft_toRightOf="@id/txtDrV1"
        app:layout_constraintBottom_toBottomOf="@id/txtDrV1" />

    <TextView
        android:id="@+id/txtDrA1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전류지령 (A)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtDrV1"
        app:layout_constraintLeft_toLeftOf="@id/txtDrV1" />

    <EditText
        android:id="@+id/editDrA1"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:text="0"
        android:gravity="right"
        android:textColor="#000000"
        android:background="@drawable/shape_edit_text_focus"
        app:layout_constraintLeft_toRightOf="@id/txtDrA1"
        app:layout_constraintBottom_toBottomOf="@id/txtDrA1" />

    <TextView
        android:id="@+id/txtTitle2"
        android:layout_width="512dp"
        android:layout_height="wrap_content"
        android:textSize="36sp"
        android:textAlignment="center"
        android:layout_marginTop="10dp"
        android:text="LOAD TEST (CH2)"
        android:background="@drawable/shape_text_title"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtTitle1" />

    <Button
        android:id="@+id/btnStart2"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:text=" Start"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_confirm_btn"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/txtTitle2"
        app:layout_constraintLeft_toLeftOf="@id/txtTitle2" />

    <Button
        android:id="@+id/btnStop2"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:text=" Stop"
        app:backgroundTint="@null"
        android:textSize="24sp"
        android:background="@drawable/selector_stop_btn"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:enabled="false"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@id/txtTitle2"
        app:layout_constraintLeft_toRightOf="@id/btnStart2" />

    <TextView
        android:id="@+id/txtOutV2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="출력전압 (V)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtDrA2"
        app:layout_constraintLeft_toLeftOf="@id/btnStart2" />

    <EditText
        android:id="@+id/editOutV2"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:gravity="right"
        android:textColor="#000000"
        android:focusable="false"
        android:background="@drawable/shape_edit_text_disenable"
        app:layout_constraintLeft_toRightOf="@id/txtOutV2"
        app:layout_constraintBottom_toBottomOf="@id/txtOutV2" />

    <TextView
        android:id="@+id/txtOutA2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="출력전류 (A)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtOutV2"
        app:layout_constraintLeft_toLeftOf="@id/txtOutV2" />

    <EditText
        android:id="@+id/editOutA2"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:gravity="right"
        android:textColor="#000000"
        android:focusable="false"
        android:background="@drawable/shape_edit_text_disenable"
        app:layout_constraintLeft_toRightOf="@id/txtOutA2"
        app:layout_constraintBottom_toBottomOf="@id/txtOutA2" />

    <ToggleButton
        android:id="@+id/btnMainMC2"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:textOff="Main MC off"
        android:textOn="Main MC on"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/editOutA2"
        app:layout_constraintLeft_toLeftOf="@id/editOutA2" />

    <TextView
        android:id="@+id/txtDrV2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전압지령 (V)"
        android:textSize="24sp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/btnStart2"
        app:layout_constraintLeft_toLeftOf="@id/btnStart2" />

    <EditText
        android:id="@+id/editDrV2"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:text="0"
        android:gravity="right"
        android:textColor="#000000"
        android:background="@drawable/shape_edit_text_focus"
        app:layout_constraintLeft_toRightOf="@id/txtDrV2"
        app:layout_constraintBottom_toBottomOf="@id/txtDrV2" />

    <TextView
        android:id="@+id/txtDrA2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전류지령 (A)"
        android:textSize="24sp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/txtDrV2"
        app:layout_constraintLeft_toLeftOf="@id/txtDrV2" />

    <EditText
        android:id="@+id/editDrA2"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:inputType="numberDecimal"
        android:text="0"
        android:gravity="right"
        android:textColor="#000000"
        android:background="@drawable/shape_edit_text_focus"
        app:layout_constraintLeft_toRightOf="@id/txtDrA2"
        app:layout_constraintBottom_toBottomOf="@id/txtDrA2" />

    <Button
        android:id="@+id/btnKeyBoard"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:text="키보드 숨기기"
        android:textSize="20sp"
        android:layout_marginStart="25dp"
        app:layout_constraintLeft_toRightOf="@id/btnMainMC2"
        app:layout_constraintTop_toTopOf="@id/editDrA2" />


</androidx.constraintlayout.widget.ConstraintLayout>